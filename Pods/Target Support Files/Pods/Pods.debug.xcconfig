CLANG_CXX_LANGUAGE_STANDARD = c++11
CLANG_CXX_LIBRARY = libc++
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/AWSAPIGateway.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/AWSCognito.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/AWSCore.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/AWSLambda.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/AWSS3.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/AsyncDisplayKit.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Bolts.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/FBSDKCoreKit.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/FBSDKLoginKit.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/ReactiveCocoa.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Realm.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/Result.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/SwiftyJSON.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "AWSAPIGateway" -framework "AWSCognito" -framework "AWSCore" -framework "AWSLambda" -framework "AWSS3" -framework "AsyncDisplayKit" -framework "Bolts" -framework "FBSDKCoreKit" -framework "FBSDKLoginKit" -framework "ReactiveCocoa" -framework "Realm" -framework "Result" -framework "SwiftyJSON"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods
PODS_ROOT = ${SRCROOT}/Pods